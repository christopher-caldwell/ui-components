{
  "name": "@caldwell619/ui-components",
  "version": "0.0.4",
  "license": "MIT",
  "homepage": "https://christopher-caldwell.github.io/ui-components",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write src",
    "build": "tsc && npx tscpaths -p tsconfig.json -s src -o dist",
    "build:watch": "nodemon --exec npm run build --watch src --ext .tsx,.ts --delay 2.5",
    "release": "standard-version",
    "publish-release": "git push --follow-tags origin master && yarn publish",
    "publish:release": "sh publish.sh",
    "predeploy": "cd demo && yarn install && yarn run build",
    "deploy": "gh-pages -d demo/dist"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=5.8.4",
    "@mui/material": ">=5.10.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.1",
    "@tanstack/react-table": "^8.5.11",
    "@types/node": "^18.7.13",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.2.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rc": "^1.2.8",
    "react": "^17.0.2",
    "react-charts": "3.0.0-beta.30",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "standard-version": "^9.3.2",
    "tscpaths": "^0.0.9",
    "typescript": "4.4.4"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ]
}
